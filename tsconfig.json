{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  },
     "include": [
        "client/src/**/*.ts",
         "client/src/app/**/*.ts"
    ],
    
     "exclude": [
        "node_modules",
        "client/node_modules"
    ]
    
}
